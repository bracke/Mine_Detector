with "../../Code/gnoga/src/gnoga.gpr";

project Mine_Detector is
   for Languages use ("Ada");
   for Source_Dirs use (".");
   for Object_Dir use ".";
   for Exec_Dir use ".";
   for Main use ("mine_detector.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-gnato2", "-gnatan", "-O2", "-fstack-check");
   end Compiler;

   package Binder is
      for Default_Switches ("ada") use ("-E", "-static");
   end Binder;
end Mine_Detector;
